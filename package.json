{
  "name": "Preserve.dev",
  "description": "Desktop application for Preserve.dev",
  "version": "0.1.1",
  "private": true,
  "author": {
    "name": "Preserve.dev",
    "email": "support@preserve.dev"
  },
  "main": ".webpack/main",
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/lab": "^5.0.0-alpha.100",
    "@mui/material": "^5.10.5",
    "@mui/x-date-pickers": "^5.0.2",
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/chroma-js": "^2.1.4",
    "async-retry": "^1.3.3",
    "axios": "^0.27.2",
    "chroma-js": "^2.4.2",
    "date-fns": "^2.29.3",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "filesize": "^9.0.11",
    "javascript-time-ago": "^2.5.7",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "memory-cache-node": "^1.2.0",
    "node-cron": "^3.0.2",
    "p-retry": "^5.1.1",
    "queue": "^6.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.4.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-saga": "^1.2.1",
    "sqlite3": "^5.1.2",
    "stopword": "^2.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4",
    "wordcloud": "^1.2.2"
  },
  "scripts": {
    "start": "NODE_ENV=development && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "test": "rm -f src/db/test.db.sqlite3 && jest --forceExit --runInBand",
    "dev": "tsc && cross-env BROWSER=none concurrently -k \"yarn start\" \"yarn electron\"",
    "electron": "electron ./distmain.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "no-loop-func": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/favicon.ico"
      },
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "cgladish",
              "name": "caught-dev"
            },
            "prerelease": true
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Preserve.dev",
            "setupIcon": "./src/assets/favicon.ico"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "additionalDMGOptions": {
              "title": "Preserve.dev",
              "icon": "./src/assets/favicon.icns"
            },
            "name": "Preserve.dev",
            "icon": "./src/assets/favicon.icns"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-dmg": "^6.0.0-beta.68",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.67",
    "@electron-forge/publisher-github": "^6.0.0-beta.68",
    "@types/async-retry": "^1.4.5",
    "@types/jest": "^29.0.3",
    "@types/lodash": "^4.14.185",
    "@types/node": "^16.11.59",
    "@types/node-cron": "^3.0.4",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/stopword": "^2.0.0",
    "@types/uuid": "^8.3.4",
    "@types/wordcloud": "^1.1.3",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "aws-sdk": "^2.1240.0",
    "concurrently": "^7.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.0.0",
    "electron": "19.1.3",
    "electron-rebuild": "^3.2.9",
    "electron-reloader": "^1.2.3",
    "electron-squirrel-startup": "^1.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "jest": "^29.0.3",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.2.9",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.2.2"
  }
}
